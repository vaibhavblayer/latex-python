\begin{center}
 \texttt{
 Algorithm for checking prime number
} 
 \end{center}
\begin{minted}[tabsize=4]{python}

def is_prime(n):
	for i in range(2, n/2 + 1, 1):
		if n % i == 0:
			return False
	else:
		return True



print(is_prime(5))

for n in range(2, 100000, 1):
	print(n, is_prime(n))
\end{minted}
